<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_ApiKey">密鑰</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_ApiKey_Placeholder">請輸入您的密鑰</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_ApiKey_Description">輸入您的 Azure 翻譯 API 密鑰</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_ApiKey_Required">請填寫 API 密鑰</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_Region">區域</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_Region_Placeholder">例如：eastus、westeurope</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_Region_Description">輸入您的 Azure 資源區域</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_Region_Required">請填寫區域</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_UseCustomEndpoint">自訂終結點</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_UseCustomEndpoint_Description">啟用以使用自訂終結點（用於主權雲）</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_CustomEndpoint">終結點地址</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_CustomEndpoint_Placeholder">https://your-endpoint.cognitiveservices.azure.com/translate</sys:String>
    <sys:String x:Key="STranslate_Plugin_Translate_AzureTranslator_CustomEndpoint_Description">輸入您的自訂終結點地址</sys:String>

</ResourceDictionary>
